import java.util.Scanner;

public class Fibonacci{
/**
 * Metodo respons√°vel por calcular o fibonacci do numero informado
 * @param n numero informado para calculo de fibonacci
 * @return retorna o numero que representa o calculo de fibonacci
 */


public static int calcularFibonacci(int n) {
    int resultado, penultimo = 1, antepenultimo = 0;
if(n<2){
    return n;
}
    for (int i = 2; i <= n; i++) {
        resultado = antepenultimo + penultimo;
        antepenultimo = penultimo;
        penultimo = resultado;
        return resultado;
    }
    //fib(3) = fib(2)/resultado + fib(1)/penultimo

}
